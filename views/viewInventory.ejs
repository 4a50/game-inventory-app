<!DOCTYPE html>
<html>
    <head>
        <%- include('./head') %>
    </head>
    <body>
        <header>
            <%- include('./goBackPartial') %>
        </header>
        <main><% something.forEach( game => { %>
            <form action="" method="">
                <h2><%= game.name %></h2>
                <section>
                    <button type="submit">View Details</button>
                </section>
            </form>
            <form action="/delete/<%= detailData.game_id %>?_method=DELETE" method="POST">
                <section>
                    <button type="submit">Delete from Inventory</button>
                </section>
            </form>
        <% }) %>
        </main>
        <footer><% include('./footerPartial') %></footer>
    </body>
</html>