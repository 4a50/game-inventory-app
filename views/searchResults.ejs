<!DOCTYPE html>
<html>
    <head>
        <%- include('./head') %>
    </head>
    <body>
        <header>
            <%- include('./goBackPartial') %>
        </header>
        <main>
            <section><% searchResultsData.forEach( game=> { %> 
                <h2><%= game.title %></h2>
                <input type="hidden" name="gameTitle" value="<%=JSON.stringify(game)%>">
                <form>
                    <button>View Details</button>
                </form>
                <form>
                    <button>Add to Inventory</button>
                </form>
                <% }) %>
            </section>
        </main>
        <footer><% include('./footerPartial') %></footer>
    </body>
</html>